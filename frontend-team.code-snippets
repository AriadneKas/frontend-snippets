	"Swiper js script": {
		"prefix": "swiper-js-script",
		"description": "Swiper js script for the swiper component to work.",
		"scope": "javascriptreact,html",
		"body": [
			"<div class=\"jq-wrapper-external\">",
			"    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/Swiper/8.4.4/swiper-bundle.min.js\"",
			"        integrity=\"sha512-k2o1KZdvUi59PUXirfThShW9Gdwtk+jVYum6t7RmyCNAVyF9ozijFpvLEWmpgqkHuqSWpflsLf5+PEW6Lxy/wA==\"",
			"        crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>",
			"</div>",
			"<link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css\" />",
			"",
			"<div class=\"jq-wrapper-internal\">",
			"    <script id=\"swiper\">",
			"        let swiperInitInterval = setInterval(checkMainSwiper, 100);",
			"",
			"        function checkMainSwiper() {",
			"            if (typeof Swiper !== \"undefined\") {",
			"                clearInterval(swiperInitInterval);",
			"                initializeMainSwiper();",
			"            }",
			"        }",
			"",
			"        function initializeMainSwiper() {",
			"            const swiper = new Swiper(\".swiper\", {",
			"                loop: false,",
			"                spaceBetween: 20,",
			"                slidesPerView: 1.1,",
			"                breakpoints: {",
			"                    700: {",
			"                        slidesPerView: 2,",
			"                    },",
			"                    1050: {",
			"                        slidesPerView: 3,",
			"                    }",
			"                },",
			"                observeParents: true,",
			"                pagination: {",
			"                    el: \".swiper-pagination\",",
			"                    clickable: true,",
			"                },",
			"                navigation: {",
			"                    prevEl: \".swiper-button-prev\",",
			"                    nextEl: \".swiper-button-next\",",
			"                },",
			"                on: {",
			"                    init: function () {",
			"                        maybeHideArrows(this);",
			"                    },",
			"                    resize: function () {",
			"                        maybeHideArrows(this);",
			"                    }",
			"                }",
			"            });",
			"        }",
			"",
			"        function maybeHideArrows(swiper) {",
			"            const totalSlides = swiper.slides.length;",
			"            const visibleSlides = swiper.params.slidesPerView;",
			"",
			"            const prevEl = document.querySelector('.swiper-button-prev');",
			"            const nextEl = document.querySelector('.swiper-button-next');",
			"",
			"            if (typeof visibleSlides === 'number' && totalSlides <= visibleSlides) {",
			"                prevEl.style.display = \"none\";",
			"                nextEl.style.display = \"none\";",
			"            } else {",
			"                prevEl.style.display = \"\";",
			"                nextEl.style.display = \"\";",
			"            }",
			"        }",
			"    </script>",
			"    <div class=\"inject\"></div>",
			"</div>",
		]
	}
