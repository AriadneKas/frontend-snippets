			"",
			"        <script id=\"int1\">",
			"            let has_counter = true;",
			"            /* ******* SET EVENT DATE ******* */",
			"            // example format: Sep 7, 2022 20:00:00",
			"            // if autoReset is TRUE this is ignored",
			"            let date = \"February 8, 2025 23:59:59\"",
			"",
			"            if (has_counter) {",
			"",
			"                let countdownCont = document.querySelector('#countdownCont')",
			"                // countdownCont.classList.toggle('preh-hide')",
			"",
			"                /* ******* SET WHAT COUNTER TO DISPLAY (true/false) ******* */",
			"                let displayDays = false",
			"                let displayHours = true",
			"                let displayMinutes = true",
			"                let displaySeconds = true",
			"",
			"                if ( !displayDays) {",
			"                    document.getElementById(\"daysNo-cont\").classList.add('preh-hide')",
			"                    document.getElementById(\"daysNo\").classList.add('preh-hide')",
			"                    document.getElementById(\"daysDivider\").classList.add('preh-hide')",
			"                    document.getElementById(\"daysText\").classList.add('preh-hide')",
			"                }",
			"",
			"                /* 24 HOUR COUNTDOWN THAT RESETS AUTOMATICALLY */",
			"                let autoReset = false;",
			"",
			"                // days",
			"                let populateUI = (days, hours, minutes, seconds) => {",
			"",
			"                    document.getElementById(\"countdown-cont\").classList.add('show');",
			"",
			"                    if (days <= 9) {",
			"                        days = \"0\" + days",
			"                    }",
			"",
			"                    if (hours <= 9) {",
			"                        hours = \"0\" + hours",
			"                    }",
			"",
			"                    if (minutes <= 9) {",
			"                        minutes = \"0\" + minutes",
			"                    }",
			"",
			"                    if (seconds <= 9) {",
			"                        seconds = \"0\" + seconds",
			"                    }",
			"",
			"",
			"                    if (hours >= 100) {",
			"                        $('#hoursNo').css('margin-right', '12px')",
			"                        $('.countdown-row-text').css('margin-left', '12px')",
			"                    }",
			"",
			"                    if (displayDays == true) {",
			"                        document.getElementById(\"daysNo\").innerHTML = days",
			"                    } else {",
			"                        document.getElementById(\"daysNo-cont\").classList.add('preh-hide')",
			"                        document.getElementById(\"daysNo\").classList.add('preh-hide')",
			"                        document.getElementById(\"daysDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"daysText\").classList.add('preh-hide')",
			"                    }",
			"",
			"                    if (displayHours == true) {",
			"                        document.getElementById(\"hoursNo\").innerHTML = hours",
			"                    } else {",
			"                        document.getElementById(\"hoursNo\").classList.add('preh-hide')",
			"                        document.getElementById(\"hoursDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"daysDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"hoursText\").classList.add('preh-hide')",
			"                    }",
			"",
			"                    if (displayMinutes == true) {",
			"                        document.getElementById(\"minutesNo\").innerHTML = minutes",
			"                    } else {",
			"                        document.getElementById(\"minutesNo\").classList.add('preh-hide')",
			"                        document.getElementById(\"minutesDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"hoursDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"minutesText\").classList.add('preh-hide')",
			"                    }",
			"                    if (displaySeconds == true) {",
			"                        document.getElementById(\"secondsNo\").innerHTML = seconds",
			"                    } else {",
			"                        document.getElementById(\"secondsNo\").classList.add('preh-hide')",
			"                        document.getElementById(\"minutesDivider\").classList.add('preh-hide')",
			"                        document.getElementById(\"secondsText\").classList.add('preh-hide')",
			"                    }",
			"                }",
			"",
			"                if (autoReset == true) {",
			"                    setInterval(time = () => {",
			"                        let day = new Date()",
			"                        let days = 23 - day.getDays()",
			"                        let hours = 59 - day.getHours()",
			"                        let minutes = 59 - day.getMinutes()",
			"                        let seconds = 60 - day.getSeconds()",
			"",
			"                        populateUI(days, hours, minutes, seconds)",
			"",
			"                    }, 1000)",
			"                } else {",
			"                    /* countdown function */",
			"                    let countdownToDate = new Date(date).getTime()",
			"",
			"                    let countdownFunc = setInterval(() => {",
			"                        let now = new Date().getTime();",
			"",
			"                        let timeleft = countdownToDate - now;",
			"",
			"                        let days = Math.floor(timeleft / (1000 * 60 * 60 * 24))",
			"                        let hours = Math.floor((timeleft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60))",
			"                        //let hours = Math.floor(timeleft / (1000 * 60 * 60))",
			"                        let minutes = Math.floor((timeleft % (1000 * 60 * 60)) / (1000 * 60))",
			"                        let seconds = Math.floor((timeleft % (1000 * 60)) / 1000)",
			"",
			"                        if (days <= 0 && hours <= 0 && minutes <= 0 && seconds <= 0) {",
			"                            document.getElementById('top-pre-header').classList.add('preh-hide')",
			"                            // document.getElementById('countdownText').classList.add('preh-hide')",
			"                            clearInterval(countdownFunc)",
			"",
			"                        } else {",
			"                            populateUI(days, hours, minutes, seconds)",
			"                        }",
			"                    }, 1000)",
			"                }",
			"            }",
			"",
			"            $(function () {",
			"                if (window.location.href.includes('kotsovolos.gr/black-friday') || window.location.href == 'https://www.kotsovolos.gr' || window.location.href == 'https://www.kotsovolos.gr/') {",
			"                    //$(\"#top-pre-header\").hide(0);",
			"                }",
			"            });",
			"        </script>",
			"",
			"    </div>"
		]
